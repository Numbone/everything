    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "i18next": "^23.2.3",
    "i18next-browser-languagedetector": "^7.0.2",
    "i18next-http-backend": "^2.2.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.12.1",
    "storybook": "^7.0.24",

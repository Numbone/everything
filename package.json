{
  "name": "eslint",
  "version": "0.01",
  "description": "middle",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve  --env port=3001 --env production",
    "project": "concurrently \"yarn start\" \"yarn back\"",
    "back": "node ./json_server/index.js",
    "build:prod": "webpack  --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "style": "npx stylelint \"**/*.scss\"",
    "style:fix": "npx stylelint \"**/*.scss\" --fix",
    "unit": "jest --config ./config/jest/jest.config.ts",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build",
    "test:ui:ci": "yarn loki --requireReference --reactUri file:./storybook-static",
    "test:ui": "yarn loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/webpack": "^8.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.1",
